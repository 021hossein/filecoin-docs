{
  "name": "ipfs-docs",
  "version": "2.0.0-beta.1",
  "description": "IPFS Docs platform",
  "author": "cwaring",
  "license": "MIT",
  "dependencies": {
    "vuepress": "^1.2.0"
  },
  "devDependencies": {
    "@vuepress/plugin-back-to-top": "^1.2.0",
    "@vuepress/plugin-last-updated": "^1.2.0",
    "husky": "^3.0.9",
    "prettier": "^1.18.2",
    "pretty-quick": "^2.0.0",
    "vuepress-plugin-clean-urls": "^1.0.3"
  },
  "prettier": {
    "arrowParens": "avoid",
    "bracketSpacing": true,
    "jsxBracketSameLine": true,
    "jsxSingleQuote": false,
    "printWidth": 80,
    "quoteProps": "as-needed",
    "semi": false,
    "singleQuote": true,
    "tabWidth": 2,
    "trailingComma": "none",
    "useTabs": false
  },
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged"
    }
  },
  "scripts": {
    "start": "npm run docs:dev",
    "test": "echo \"Error: no test specified\" && exit 1",
    "docs:dev": "vuepress dev docs",
    "docs:build": "vuepress build docs"
  }
}
